{
  "name": "@pooltogether/v4-pool-data",
  "author": "PoolTogether Inc.",
  "module": "dist/v4-pool-data.esm.js",
  "version": "0.9.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=16"
  },
  "repository": {
    "url": "https://github.com/pooltogether/v4-pool-data",
    "type": "git"
  },
  "scripts": {
    "start": "tsdx watch --onSuccess \"yalc push\"",
    "reinstall": "rm -rf node_modules && yarn install",
    "postbuild": "cp src/testnet.json dist/testnet.json",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "yarn build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 100,
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true,
    "quoteProps": "consistent",
    "jsxBracketSameLine": false,
    "jsxSingleQuote": true,
    "arrowParens": "always",
    "trailingComma": "none"
  },
  "size-limit": [
    {
      "path": "dist/v4-pool-data.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/v4-pool-data.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@pooltogether/contract-list-schema": "^0.1.3",
    "@size-limit/preset-small-lib": "^5.0.3",
    "ajv": "^8.6.2",
    "husky": "^7.0.2",
    "size-limit": "^5.0.3",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^4.4.2"
  },
  "dependencies": {},
  "peerDependencies": {}
}
