{
  "name": "@pooltogether/v4-pool-data",
  "author": "PoolTogether Inc.",
  "version": "1.8.0-beta.4",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "module": "dist/v4-pool-data.esm.js",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "repository": {
    "url": "https://github.com/pooltogether/v4-pool-data",
    "type": "git"
  },
  "scripts": {
    "start": "tsdx watch --onSuccess \"yalc push\"",
    "reinstall": "rm -rf node_modules && yarn install",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "format": "prettier --write \"**/*.{ts,js}\"",
    "prepare": "yarn build"
  },
  "devDependencies": {
    "@pooltogether/contract-list-schema": "^0.1.3",
    "ajv": "^8.6.2",
    "husky": "^7.0.2",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^4.4.2"
  },
  "dependencies": {
    "@pooltogether/v4-mainnet": "1.6.0",
    "@pooltogether/v4-testnet": "1.5.4"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "yarn format && tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 100,
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true,
    "quoteProps": "consistent",
    "jsxBracketSameLine": false,
    "jsxSingleQuote": true,
    "arrowParens": "always",
    "trailingComma": "none"
  }
}
